                lineColors: ['#ffb573', '#73abff'],
                lineWidth: '1px',
                resize: true,
                redraw: true
            });

            window.lineChart = Morris.Line({
                element: 'line-chart3',
                data: [
                    { y: '2006', a: 100, b: 90 },
                    { y: '2007', a: 75, b: 65 },
                    { y: '2008', a: 50, b: 40 },
